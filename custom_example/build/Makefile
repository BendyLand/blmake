# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Full_build_config = debug

else ifeq ($(config),release)
  Full_build_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Full_build

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Full_build:
ifneq (,$(Full_build_config))
	@echo "==== Building Full_build ($(Full_build_config)) ===="
	@${MAKE} --no-print-directory -C . -f Full_build.make config=$(Full_build_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Full_build.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Full_build"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"